<?xml version="1.0" encoding="UTF-8" ?>
<!DOCTYPE mapper PUBLIC "-//mybatis.org//DTD Mapper 3.0//EN" "http://mybatis.org/dtd/mybatis-3-mapper.dtd" >
<mapper namespace="com.grant.research.api.idempotent.dao.IdempotentDao">

    <!-- 查询剩余 -->
    <select id="getById" resultType="com.grant.research.api.idempotent.entity.Idempotent">
        SELECT id,over_num AS overNum from research_idempotent where id = #{id}
    </select>
</mapper>
